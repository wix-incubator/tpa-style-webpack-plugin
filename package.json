{
  "name": "tpa-style-webpack-plugin",
  "version": "1.0.4",
  "description": "A Webpack plugin that handles wix tpa styles, it separates static css file that injects dynamic style at runtime.",
  "main": "dist/lib/index.js",
  "scripts": {
    "lint": "tslint -t stylish -p .",
    "pretest": "rm -rf dist && npm run lint && tsc -b",
    "test": "jest"
  },
  "keywords": [
    "Wix",
    "TPA",
    "style",
    "Webpack",
    "plugin"
  ],
  "files": [
    "dist",
    "*.js"
  ],
  "author": "Felix Mosheev",
  "license": "MIT",
  "repository": "https://github.com/wix-incubator/tpa-style-webpack-plugin",
  "homepage": "https://github.com/wix-incubator/tpa-style-webpack-plugin",
  "dependencies": {
    "@ctrl/tinycolor": "^2.2.1",
    "parse-css-font": "^3.0.2",
    "postcss": "^7.0.5",
    "postcss-extract-styles": "^1.2.0",
    "postcss-prefix-selector": "^1.6.0",
    "webpack": "^4.20.2",
    "webpack-sources": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-teamcity-reporter": "^0.9.0",
    "mini-css-extract-plugin": "^0.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-yoshi-base": "^3.12.0",
    "typescript": "^3.1.1",
    "webpack-merge": "^4.1.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testResultsProcessor": "jest-teamcity-reporter",
    "testRegex": "(/tests/units/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ]
  }
}
